<script>    
    $("#telefono").keypress(function(){
     $('#telefono-m').html('');
      var ok = true;      
    }); 
  
      $("#nombre").keypress(function(){
       $('#nombre-m').html('');
     	var ok = true;        
      }); 
  
  	   $(".next").click(function(){	          
       var ok = true;
          
       $('#nombre-m').html('');
       $('#telefono-m').html('');
       $(".message").html('');   
          
  		var regex = /[\w-\.]{2,}@([\w-]{2,}\.)*([\w-]{2,}\.)[\w-]{2,4}/;          
          if ($('#nombre').val()=='') {   
              $('#nombre-m').show();
              $('#nombre-m').html('Rellena este campo obligatorio.');
            var ok = false;
        }          
		
          if ($('#telefono').val().length <= 9 || isNaN($('#telefono').val())) {
            $('#telefono-m').show();
            $('#telefono-m').html('Este campo es inválido');
            var ok = false;
          }
          if (ok == false)
            {
              return false;
            }
          
          console.log($('#telefono').val().lengt);
    	if (regex.test($("#email").val().trim())) {
		  var telefono= $("#telefono").val();
		  var email= $("#email").val();
		  var nombre= $("#nombre").val();

		  var sEmails=email.split("@");
		  var  use=sEmails[0];
		  var domain=sEmails[1]; 	
		  
           if (domain =='prodigy.net.mx' || domain =='gmail.com' || domain =='hotmail.com' || domain =='outlook.com' || domain =='yahoo.com.mx' || domain =='icloud.com' || domain =='live.com'  || domain =='hotmail.es'  || domain =='outlook.es' || domain =='yahoo.com' || domain =='msn.com ') {
			 $(".message").html("Tu correo debe ser empresarial");          
             return false;			 
		   } 		  	  
	
		  
         if (nombre==''){
			 $(".message").html("El nombre es requerido");
			 }		 
			 else { 	  
			 $('#formulario-2-pasos').hide('slow');   
             $('div#formulario-2').show('slow');                  
               
			   hbspt.forms.create({
						portalId: "2039173",
						formId: '93a431f3-17e6-4d22-92c2-51bb9e303341',
						css: "",
						target: "#formulario-2",
						onFormReady: function(){
						 $('input[name="firstname"]').val(nombre).change(); 						
						 $('input[name="phone"]').val(telefono).change();  
						 $('input[name="email"]').val(email).change(); 						 
						}
					 });
				 }

    } else {
      $(".message").show();
      $(".message").html("Dirección de correo inválida");
      return false;
    }        
   				});
</script>
    
      
                
    
  
